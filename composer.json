{
    "name": "Cranux/laravel-dotenv-editor",
    "description": "A package for editing the .env file in your Laravel root.",
    "keywords": ["laravel", "dotenv", "dotenv-editor"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "cranux",
            "email": "tuplelist@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.0"
    },
    "autoload": {
        "psr-4": {
            "Cranux\\DotenvEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cranux\\DotenvEditor\\DotenvEditorServiceProvider"
            ],
            "aliases": {
                "DotenvEditor": "Cranux\\DotenvEditor\\DotenvEditorFacade"
            }
        }
    }
}
